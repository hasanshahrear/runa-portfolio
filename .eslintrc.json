{
  "extends": [
    "next/core-web-vitals",
    "airbnb",
    "airbnb-typescript",
    "plugin:import/typescript"
  ],
  "parserOptions": {
    "project": "./tsconfig.json"
  },
  "plugins": ["react", "@typescript-eslint", "prettier"],
  "ignorePatterns": ["next.config.js", "postcss.config.js"],
  "rules": {
    "react/react-in-jsx-scope": "off"
  }
}
